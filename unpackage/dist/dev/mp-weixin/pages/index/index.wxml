<view class="container"><view class="vertical-section-box" style="height:100%;"><view class="section-middle msg-container"><block wx:for="{{descList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="msg-item"><view class="log"></view><view class="content">{{item.content}}</view></view></block><view class="msg-item" style="height:130rpx;align-items:center;"><view class="log"></view><view class="content"><radio-group data-event-opts="{{[['change',[['changeVersion',['$event']]]]]}}" bindchange="__e"><block wx:for="{{versionList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label style="display:inline-block;"><view class="{{[(index!==0)?'mgr':'']}}"><radio style="transform:scale(0.7);" value="{{item.value}}" checked="{{item.value===selectedVersion}}"></radio><text>{{item.content}}</text></view></label></block></radio-group></view></view></view><view class="section-bottom"><view data-event-opts="{{[['tap',[['goTest',['$event']]]]]}}" class="common-btn btn" bindtap="__e">开始测试</view></view></view></view>